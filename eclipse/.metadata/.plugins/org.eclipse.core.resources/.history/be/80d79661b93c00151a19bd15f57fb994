package com.skeeter144.crafting;

import com.skeeter144.init.ModItems;

import net.minecraft.init.Blocks;
import net.minecraft.init.Items;
import net.minecraft.item.ItemStack;
import cpw.mods.fml.common.registry.GameRegistry;

public class Recipes {

	public static void init(){
		GameRegistry.addShapedRecipe(new ItemStack(ModItems.makeshiftHatchet,1), new Object[]
				{"AB", "C", 'A', Blocks.cobblestone, 'B', Blocks.vine, 'C', Items.stick});
	}
}
